{
    "manifest_version": 2,

    "name": "Cookie Manager",
    "description": "Manage your browser cookies",
    "version": "1.0",

    "options_page": "options.html",
    "background": {
        "scripts": ["contextmenu.js"]
    },
    "permissions": [
        "cookies",
        "contextMenus",
        "<all_urls>"
    ]
}
